<h2>
    Command to create CanActivate route guard in Angular.
</h2>

<p>
    Command Syntax :- <br>

    ng generate guard RouteGuards/RG1 --implements CanActivate <br>

    ng g guard RouteGuards/RG2 --implements CanActivate <br>
 
    ng g g RouteGuards/RG2 --implements CanActivate <br>

</p>

import { CanActivateFn } from '@angular/router';

export const rG1Guard: CanActivateFn = (route, state) => {
  return true;
};
