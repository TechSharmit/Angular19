<h2>
    Command to create CanActivateChild Route in Angular.
</h2>

<p>
    Commands :- <br>

    Command-1  : ng generate guard RouteGuards/RG1 --implements CanActivateChild

    Command-2  : ng g guard RouteGuards/RG2 --implements CanActivateChild

    Command-3  : ng g g RouteGuards/RG3 --implements CanActivateChild
</p>


import { CanActivateChildFn } from '@angular/router';

export const rG1Guard: CanActivateChildFn = (childRoute, state) => {
  return true;
};
